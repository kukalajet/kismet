{
	"lsp": {
		"deno": {
			"settings": {
				"deno": {
					"enable": true,
					"lint": true
				}
			}
		}
	},
	"languages": {
		"TypeScript": {
			"formatter": "language_server",
			"language_servers": [
				"deno",
				"!typescript-language-server",
				"!vtsls",
				"!eslint"
			]
		},
		"TSX": {
			"formatter": "language_server",
			"language_servers": [
				"deno",
				"!typescript-language-server",
				"!vtsls",
				"!eslint"
			]
		},
		"JavaScript": {
			"formatter": "language_server",
			"language_servers": [
				"deno",
				"!typescript-language-server",
				"!vtsls",
				"!eslint"
			]
		}
	}
}
